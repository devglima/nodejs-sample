{
   "presets": [
      [
         "@babel/preset-env",
         {
            "targets": {
               "node": "current"
            }
         }
      ],
   ],
   "plugins": [
      [
         "module-resolver",
         {
            "root": [
               "./src"
            ],
            "alias": {
               "App": "./src",
               "Model": "./Models",
               "Controller": "./controllers"
            }
         }
      ]
   ],
   "ignore": [
      "**/*.spec.ts"
   ]
}